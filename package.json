        "@remix-run/node": "^2.15.1",
        "@remix-run/react": "^2.15.1",
        "@remix-run/serve": "^2.15.1",
        "axios": "^1.7.9",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "isbot": "^4.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.4.0",
        "react-redux": "^9.1.2",
        "react-time-picker": "^7.0.0",
        "remix-utils": "^7.7.0",
        "rsuite": "^5.75.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.0",
